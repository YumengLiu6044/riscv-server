{
	"name": "riscv-assembly-language-support",
	"displayName": "riscv-assembly-language-support",
	"description": "RISCV Assembly syntax highlighting and execution",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.104.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "riscv-assembly-language-support.helloWorld",
				"title": "Hello World"
			}
		],
		"languages": [
			{
				"id": "riscv",
				"aliases": [
					"RISC-V",
					"riscv"
				],
				"extensions": [
					".S",
					".s"
				],
				"configuration": "./riscv-language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "riscv",
				"scopeName": "source.riscv",
				"path": "./syntaxes/riscv.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.104.0",
		"@types/mocha": "^10.0.10",
		"@types/node": "22.x",
		"@typescript-eslint/eslint-plugin": "^8.45.0",
		"@typescript-eslint/parser": "^8.45.0",
		"eslint": "^9.36.0",
		"typescript": "^5.9.3",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2"
	}
}
